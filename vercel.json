{
  "version": 2,
   "build": {
    "includeFiles": [
      "node_modules/tesseract.js-core/tesseract-core-simd.wasm"
    ],
    "env": {
      "BUILD_COMMAND": "cp node_modules/tesseract.js-core/tesseract-core-simd.wasm dist/ && npm run build"
    }
  },
  "builds": [
    {
      "src" : "scripts/generateServiceAccount.js",
      "use" : "@vercel/node",
      "config": { "includeFiles": ["scripts/**", "dist/**", "package.json"] }
    },
    {
      "src": "dist/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ]
}