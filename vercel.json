{
  "version": 2,
  "builds": [
    {
      "src" : "scripts/generateServiceAccount.js",
      "use" : "@vercel/node",
      "config": { "includeFiles": ["scripts/**", "dist/**", "package.json"] }
    },
    {
      "src": "dist/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "functions": {
    "dist/src/services/ocrService.js": {
      "includeFiles": "node_modules/tesseract.js-core/tesseract-core-simd.wasm",
      "memory": 1024,
      "maxDuration": 60
    }
  }
}